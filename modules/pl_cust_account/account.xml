        <field name="name">pl_balance_sheet_context_form</field>
        <field name="priority" eval="60"/>
    </record>

    <record model="ir.ui.view" id="plbalance_sheet_comp_context_view_form">
        <field name="model">account.balance_sheet.comparison.context</field>
